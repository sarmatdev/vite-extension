{
  "name": "Vite Wallet",
  "description": "Vite Browser Extension Wallet",
  "author": "CryptoGboors",
  "version": "0.1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Vite Wallet",
    "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "index.html"
  ]
}